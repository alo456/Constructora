<?php

namespace App\Entity;

/**
 * MaterialesPorServicio
 */
class MaterialesPorServicio
{
    /**
     * @var string
     */
    private $id;

    /**
     * @var float
     */
    private $subtotal;

    /**
     * @var \App\Entity\Orden
     */
    private $orden;

    /**
     * @var \App\Entity\Recurso
     */
    private $material;

    /**
     * @var \App\Entity\Envio
     */
    private $envio;

    /**
     * @var \App\Entity\Servicio
     */
    private $servicio;


    /**
     * Set id.
     *
     * @param string $id
     *
     * @return MaterialesPorServicio
     */
    public function setId($id)
    {
        $this->id = $id;

        return $this;
    }

    /**
     * Get id.
     *
     * @return string
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set subtotal.
     *
     * @param float $subtotal
     *
     * @return MaterialesPorServicio
     */
    public function setSubtotal($subtotal)
    {
        $this->subtotal = $subtotal;

        return $this;
    }

    /**
     * Get subtotal.
     *
     * @return float
     */
    public function getSubtotal()
    {
        return $this->subtotal;
    }

    /**
     * Set orden.
     *
     * @param \App\Entity\Orden|null $orden
     *
     * @return MaterialesPorServicio
     */
    public function setOrden(\App\Entity\Orden $orden = null)
    {
        $this->orden = $orden;

        return $this;
    }

    /**
     * Get orden.
     *
     * @return \App\Entity\Orden|null
     */
    public function getOrden()
    {
        return $this->orden;
    }

    /**
     * Set material.
     *
     * @param \App\Entity\Recurso|null $material
     *
     * @return MaterialesPorServicio
     */
    public function setMaterial(\App\Entity\Recurso $material = null)
    {
        $this->material = $material;

        return $this;
    }

    /**
     * Get material.
     *
     * @return \App\Entity\Recurso|null
     */
    public function getMaterial()
    {
        return $this->material;
    }

    /**
     * Set envio.
     *
     * @param \App\Entity\Envio|null $envio
     *
     * @return MaterialesPorServicio
     */
    public function setEnvio(\App\Entity\Envio $envio = null)
    {
        $this->envio = $envio;

        return $this;
    }

    /**
     * Get envio.
     *
     * @return \App\Entity\Envio|null
     */
    public function getEnvio()
    {
        return $this->envio;
    }

    /**
     * Set servicio.
     *
     * @param \App\Entity\Servicio|null $servicio
     *
     * @return MaterialesPorServicio
     */
    public function setServicio(\App\Entity\Servicio $servicio = null)
    {
        $this->servicio = $servicio;

        return $this;
    }

    /**
     * Get servicio.
     *
     * @return \App\Entity\Servicio|null
     */
    public function getServicio()
    {
        return $this->servicio;
    }
}
